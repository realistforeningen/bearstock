#!/bin/sh
# coding: utf-8

# shell block for setting the pythonpath
''':'
PYTHONPATH=. exec python2 "$0" "$@"
'''

# python script starts here

from web.app import app
from stock import Exchange
import subprocess

Exchange.run_in_thread()
subprocess.call(
    ["uwsgi", "--http", "0.0.0.0:5000", "--wsgi-file", "web/app.py", "--callable", "app"]
)
